{
    "name": "micahcraft",
    "data_pack": {
        "load": ["src"],
        "name": "micahcraft"
    },
    "resource_pack": {
        "load": ["resources"],
        "name": "micahcraft_resources"
    },
    "minecraft": "1.21.11",
    "output": "build",
    "author": "Micalobia",
    "version": "26.1.1",
    "description": "Adds some vanilla+ recipes and features",
    "templates": ["templates"],
    "require": [],
    "pipeline": [
        "beet.contrib.vanilla.load_vanilla",
        "tools.meta.csv",
        "tools.stonecutter.variants.run",
        "tools.stonecutter.woodcutter.run",
        "tools.stonecutter.paintings.run",
        "tools.stonecutter.unwaxed.run",
        "tools.colorable.run",
        "tools.recipe_advancements.run",
        "tools.tweaks.main.run",
        "tools.functions.score_triggers.run",
        "tools.functions.veinmining",
        "tools.json.enchantment.run",
        "tools.functions.unreachable.run",
        "tools.trims.run",
        "tools.utility.print_finished",
        "beet.contrib.format_json"
    ],
    "meta": {
        "unreachable": ["micahcraft:commands/hud/theme/(\\d+|same)"],
        "format_json": {
            "indent": 4
        },
        "csv": { "manifest": "config/csv_manifest.csv" }
    }
}
